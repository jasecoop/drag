@font-face {
  font-family: 'GTCinetype';
  font-weight: 100;
  src: url('<%= asset_path("GT-Cinetype-Trial-Light.eot") %>'),
  url('<%= asset_path("GT-Cinetype-Trial-Light.woff") %>') format('woff'),
  url('<%= asset_path("GT-Cinetype-Trial-Light.svg#GTCinetypeTrial-Light") %>') format('svg');
}

